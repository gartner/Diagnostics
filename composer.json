{
    "name": "palustris/diagnostics",
    "description": "Use ZFDiagnostics in a Zend Framework 3 application.",
    "keywords": [
        "zf3",
        "tool",
        "Zend Framework 3",
        "ZFDiagnostics"
    ],
    "homepage": "https://github.com/gartner/Diagnostics/",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "ZFTool\\": "src/"
        }
    },
    "require": {
        "php": ">=7.3.0",
        "laminas/laminas-diagnostics": "^1.3",
        "laminas/laminas-config": "^3.2",
        "laminas/laminas-console": "^2.7",
        "laminas/laminas-modulemanager": "^2.8",
        "laminas/laminas-mvc": "^3.1 || ^4.0.x-dev",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-stdlib": "^3.2",
        "laminas/laminas-view": "^2.10",
        "laminas/laminas-http": "^2.8",
        "laminas/laminas-mvc-console": "^1.2",
        "laminas/laminas-validator": "^2.10",
        "laminas/laminas-escaper": "^2.6",
        "laminas/laminas-loader": "^2.6",
        "laminas/laminas-eventmanager": "^3.2",
        "laminas/laminas-router": "^3.2 || ^4.0",
        "laminas/laminas-text": "^2.7",
        "laminas/laminas-uri": "^2.6",
        "laminas/laminas-dependency-plugin": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "bin": [
        "zf.php"
    ]
}
